AddHandler mod_python .py
PythonDebug On
PythonHandler listings

<FilesMatch ".+">
   SetHandler mod_python
</FilesMatch>

<Files index.html>
   SetHandler text_html
</Files>

<FilesMatch "\.css">
   SetHandler text_css
</FilesMatch>

<FilesMatch "\.ttf">
   SetHandler None
</FilesMatch>

<FilesMatch "\.png">
   SetHandler None
</FilesMatch>

SetHandler None
